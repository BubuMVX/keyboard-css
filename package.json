{
  "name": "@keyboard-css/core",
  "version": "0.0.0-development",
  "description": "A CSS to show keyboard keys.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "commit": "cz",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "gulp watch",
    "build": "gulp",
    "postbuild": "cpx \"{README.md,package.json}\" \"dist\"",
    "pack": "npm pack --prefix=dist",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shhdharmen/keyboard-css.git"
  },
  "keywords": [],
  "author": "Dharmen Shah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shhdharmen/keyboard-css/issues"
  },
  "homepage": "https://github.com/shhdharmen/keyboard-css#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "autoprefixer": "^10.0.1",
    "browser-sync": "^2.26.13",
    "cpx": "^1.5.0",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.3.0",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-flatmap": "^1.0.2",
    "gulp-header": "^2.0.9",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-svgmin": "^3.0.0",
    "gulp-terser": "^1.4.0",
    "husky": "^4.3.0",
    "lazypipe": "^1.0.2",
    "lint-staged": "^10.5.0",
    "markdownlint-cli": "^0.24.0",
    "node-sass": "^4.14.1",
    "postcss": "^8.1.4",
    "prettier": "2.1.2",
    "semantic-release": "^17.2.1",
    "stylelint": "^13.7.2",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-high-performance-animation": "^1.5.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": "stylelint --fix",
    "*.scss": "stylelint --syntax=scss --fix",
    "*.{scss,css}": "prettier --write",
    "*.md": "markdownlint"
  },
  "dependencies": {},
  "publishConfig": {
    "access": "restricted"
  }
}
